{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "canadacentral"
        },
        "networkInterfaceName": {
            "value": "linuxos-vm289"
        },
        "networkSecurityGroupName": {
            "value": "linuxos-vm-nsg"
        },
        "networkSecurityGroupRules": {
            "value": [
                {
                    "name": "HTTP",
                    "properties": {
                        "priority": 300,
                        "protocol": "TCP",
                        "access": "Allow",
                        "direction": "Inbound",
                        "sourceAddressPrefix": "*",
                        "sourcePortRange": "*",
                        "destinationAddressPrefix": "*",
                        "destinationPortRange": "80"
                    }
                },
                {
                    "name": "SSH",
                    "properties": {
                        "priority": 320,
                        "protocol": "TCP",
                        "access": "Allow",
                        "direction": "Inbound",
                        "sourceAddressPrefix": "*",
                        "sourcePortRange": "*",
                        "destinationAddressPrefix": "*",
                        "destinationPortRange": "22"
                    }
                }
            ]
        },
        "subnetName": {
            "value": "default"
        },
        "virtualNetworkName": {
            "value": "windowos-vm-vnet"
        },
        "addressPrefixes": {
            "value": [
                "10.0.0.0/16"
            ]
        },
        "subnets": {
            "value": [
                {
                    "name": "default",
                    "properties": {
                        "addressPrefix": "10.0.0.0/24"
                    }
                }
            ]
        },
        "publicIpAddressName": {
            "value": "linuxos-vm-ip"
        },
        "publicIpAddressType": {
            "value": "Static"
        },
        "publicIpAddressSku": {
            "value": "Standard"
        },
        "pipDeleteOption": {
            "value": "Detach"
        },
        "virtualMachineName": {
            "value": "linuxos-vm"
        },
        "virtualMachineComputerName": {
            "value": "linuxos-vm"
        },
        "virtualMachineRG": {
            "value": "template-rg"
        },
        "osDiskType": {
            "value": "Premium_LRS"
        },
        "osDiskDeleteOption": {
            "value": "Delete"
        },
        "virtualMachineSize": {
            "value": "Standard_B2s"
        },
        "nicDeleteOption": {
            "value": "Detach"
        },
        "hibernationEnabled": {
            "value": false
        },
        "adminUsername": {
            "value": "cloudadmin"
        },
        "enablePeriodicAssessment": {
            "value": "ImageDefault"
        },
        "securityType": {
            "value": "TrustedLaunch"
        },
        "secureBoot": {
            "value": true
        },
        "vTPM": {
            "value": true
        }
    }
}